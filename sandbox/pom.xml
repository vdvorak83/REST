		<artifactId>rest-parent</artifactId>
		<version>0.12-SNAPSHOT</version>
		<relativePath>..</relativePath>
	</parent>

	<dependencies>

		<!-- internal -->

		<dependency>
			<groupId>org</groupId>
			<artifactId>rest-common</artifactId>
			<version>0.12-SNAPSHOT</version>
		</dependency>

		<dependency>
			<groupId>org</groupId>
			<artifactId>rest-test</artifactId>
			<version>0.12-SNAPSHOT</version>
			<scope>test</scope>
		</dependency>
		
		<!-- aws -->

		<dependency>
